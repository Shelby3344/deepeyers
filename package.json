{
  "name": "deepeyes",
  "version": "1.0.0",
  "description": "DeepEyes Security AI Platform",
  "scripts": {
    "dev": "npx tailwindcss -i ./resources/css/app.css -o ./public/css/tailwind.css --watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "npx tailwindcss -i ./resources/css/app.css -o ./public/css/tailwind.css --minify && node scripts/minify-css.js",
    "build:js": "node scripts/minify-js.js",
    "minify": "npm run build"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "clean-css": "^5.3.3",
    "terser": "^5.27.0"
  }
}
